<p id="notice"><%= notice %></p>

<h1>Score Papers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Studentid</th>
      <th>Score</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @score_papers.each do |score_paper| %>
      <tr>
        <td><%= score_paper.name %></td>
        <td><%= score_paper.studentID %></td>
        <td><%= score_paper.score %></td>
        <td><%= link_to 'Show', score_paper %></td>
        <td><%= link_to 'Edit', edit_score_paper_path(score_paper) %></td>
        <td><%= link_to 'Destroy', score_paper, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Score Paper', new_score_paper_path %>
